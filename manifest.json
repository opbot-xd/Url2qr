{
    "name":"URL2PDF",
    "version":"0.0.1",
    "manifest_version":2,
    "browser_action":{
        "default_popup":"default.html",
        "default_icon":"pngegg.png"
    },
    "icons":{
        "128":"pngegg.png",
        "256":"256.png"
    },
    "permissions":["activeTab"],
    "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/gh/davidshimjs/qrcodejs/qrcode.min.js; object-src 'self'"
    
}